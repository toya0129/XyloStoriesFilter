function out = XyloStoriesFilter(inputData)
    tes = zeros(8,1);
    out = 0;
    
    %C1
    bandpass = fir1(48,[0.4 0.42]);
    %bandpass = fir1(48,[0.045 0.05]);
    band = filter(bandpass,1,inputData);
    for i = 1:length(band)
        if band(i,1) >= 30
            out = 1;
        end
    end
    
    %D
    bandpass = fir1(48,[0.44 0.5]);
    %bandpass = fir1(48,[0.052 0.056]);
    band = filter(bandpass,1,inputData);
    for i = 1:length(band)
        if band(i,1) >= 30
            if out ~= 2
                out = 2;
            end
        end
    end
    
    
    %E
    bandpass = fir1(48,[0.51 0.54]);
    %bandpass = fir1(48,[0.059 0.061]);
    band = filter(bandpass,1,inputData);
    for i = 1:length(band)
        if band(i,1) >= 30
            tes(3,1) = 3;
        end
    end
    
    %F
    if out == 0
        bandpass = fir1(48,[0.55 0.58]);
        band = filter(bandpass,1,inputData);
        for i = 1:length(band)
            if band(i,1) >= 30
                out = 4;
                %break;
            end
        end
    end
    
    %G
    if out == 0
        bandpass = fir1(48,[0.6 0.64]);
        band = filter(bandpass,1,inputData);
        for i = 1:length(band)
            if band(i,1) >= 30
                out = 5;
                break;
            end
        end
    end
    
    %A
    if out == 0
        bandpass = fir1(48,[0.68 0.72]);
        band = filter(bandpass,1,inputData);
        for i = 1:length(band)
            if band(i,1) >= 30
                out = 6;
                break;
            end
        end
    end
    
    %B
    if out == 0
        bandpass = fir1(48,[0.76 0.8]);
        band = filter(bandpass,1,inputData);
        for i = 1:length(band)
            if band(i,1) >= 30
                out = 7;
                break;
            end
        end
    end
    
    %C2
    if out == 0
        bandpass = fir1(48,[0.82 0.84]);
        band = filter(bandpass,1,inputData);
        for i = 1:length(band)
            if band(i,1) >= 30
                out = 8;
                break;
            end
        end
    end
    
end